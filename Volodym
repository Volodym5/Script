-- Function to sort table keys alphabetically
local function getSortedKeys(t)
    local keys = {}
    for key in pairs(t) do
        table.insert(keys, key)
    end
    table.sort(keys, function(a, b) return a:lower() < b:lower() end)
    return keys
end

-- Function to create a dropdown and button for each game
local function addGameSection(gameName, scripts)
    local selectedScript = nil -- Variable to store the selected script

    local scriptTitles = {}
    for _, script in ipairs(scripts) do
        table.insert(scriptTitles, script.Title)
    end

    local GameDropdown = Tabs.Main:AddDropdown(gameName .. " Selector", {
        Title = gameName, -- Use the game name as the dropdown title
        Values = scriptTitles,
        Multi = false,
        Default = 1, -- Set a default selection if required
    })

    -- Listen for changes in dropdown selection
    GameDropdown:OnChanged(function(value)
        selectedScript = value
    end)

    Tabs.Main:AddButton({
        Title = "Execute Script",
        Description = "",
        Callback = function()
            if not selectedScript then
                Fluent:Notify({
                    Title = "Error",
                    Content = "Please select a script before executing!",
                    Duration = 3,
                })
                return
            end

            for _, script in ipairs(scripts) do
                if script.Title == selectedScript then
                    loadstring(game:HttpGet(script.URL))()
                    return
                end
            end

            Fluent:Notify({
                Title = "Error",
                Content = "No matching script found!",
                Duration = 3,
            })
        end,
    })
end

-- Add all games and their scripts in alphabetical order
local sortedGameNames = getSortedKeys(ScriptData)
for _, gameName in ipairs(sortedGameNames) do
    local scripts = ScriptData[gameName]
    addGameSection(gameName, scripts)
end

-- Notify when the script hub is loaded
Fluent:Notify({
    Title = "Fluent",
    Content = "The script has been loaded.",
    Duration = 3,
})

SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)
SaveManager:IgnoreThemeSettings()
SaveManager:SetIgnoreIndexes({})
InterfaceManager:SetFolder("FluentScriptHub")
SaveManager:SetFolder("FluentScriptHub/specific-game")
InterfaceManager:BuildInterfaceSection(Tabs.Settings)
SaveManager:BuildConfigSection(Tabs.Settings)
Window:SelectTab(1)
SaveManager:LoadAutoloadConfig()
